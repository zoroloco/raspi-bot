{
  "swagger": "2.0",
  "info": {
    "title": "raspi-bot API documentation.",
    "description": "",
    "version": "1.0"
  },
  "produces": ["application/json"],
  "host": "localhost:7482",
  "basePath": "/",
  "paths": {
    "/connect": {
      "get":{
        "x-swagger-router-controller": "middleware-name1",
        "operationId": "swagTest",
        "tags": ["/get"],
        "description": "Sends a remote connect message that turns the blue LED indicator on."
      }
    },
    "/move": {
      "post": {
        "x-swagger-router-controller": "middleware-name1",
        "operationId": "swagTest",
        "tags": ["/move"],
        "description": "Moves servo to a position.",
        "parameters": [
          {
            "name": "servo",
            "in": "formData",
            "description": "Servo 0,1,3,4,5,6",
            "required": true,
            "type": "string"
          },
          {
            "name": "pos",
            "in": "formData",
            "description": "Position with range: 3000-9000.",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {},
        "consumes": "- application/json"
      }
    }
  }
}